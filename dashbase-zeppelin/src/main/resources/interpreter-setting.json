[
  {
    "group": "dashbase",
    "name": "dashbase",
    "className": "io.dashbase.zeppelin.DashbaseInterpreter",
    "properties": {
      "dashbase.host": {
        "envName": "DASHBASE_HOST",
        "propertyName": "dashbase.host",
        "defaultValue": "localhost",
        "description": "Dashbase host. Default = localhost"
      },
      "dashbase.host": {
        "envName": "DASHBASE_PORT",
        "propertyName": "dashbase.port",
        "defaultValue": "9876",
        "description": "Dashbase port. Default = 9876"
      },
      "use.https": {
        "envName": "USE_HTTPS",
        "propertyName": "use.https",
        "defaultValue": "false",
        "description": "Use HTTPS. Default = false"
      },
      "dashbase.connection.timeout": {
        "envName": "DASHBASE_CONNECTION_TIMEOUT",
        "propertyName": "dashbase.connection.timeout",
        "defaultValue": "10000",
        "description": "Connection timeout in milliseconds. Default = 10000"
      },
      "dashbase.socket.timeout": {
        "envName": "DASHBASE_SOCKET_TIMEOUT",
        "propertyName": "dashbase.socket.timeout",
        "defaultValue": "60000",
        "description": "Socket timeout in milliseconds. Default = 60000"
      }
    },
    "editor": {
      "editOnDblClick": false
    }
  }
]